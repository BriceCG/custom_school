<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="view_wizad_skill_history_tree" model="ir.ui.view">
            <field name="name">view.wizard.skill.history.tree</field>
            <field name="model">student.skill.history</field>
            <field name="arch" type="xml">
                <tree string="" create="0">
                    <field name="date"/>
                    <field name="skill_id" />
                    <field name="rate_number" />
                    <field name="rating" widget="priority" />
                </tree>
            </field>
        </record>

       <record id="action_wizard_skill_history_view" model="ir.actions.act_window">
           <field name="name">Skill History</field>
           <field name="type">ir.actions.act_window</field>
           <field name="res_model">student.skill.history</field>
           <field name="view_mode">tree</field>
           <field name="help" type="html">
             <p class="oe_view_nocontent_create">

             </p><p>

             </p>
           </field>
       </record>


    </data>
</odoo>